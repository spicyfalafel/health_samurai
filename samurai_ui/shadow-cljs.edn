;; shadow-cljs configuration
{:source-paths
 ["src/dev"
  "src/main"
  "src/test"]

 :dependencies
 [[reagent "1.1.0"]
  [cljs-http "0.1.46"]
  [cheshire "5.10.2"]]

 :dev-http {8000
            {:root "public"}}

 :builds
 {
  :app
  {:target :browser
   :output-dir "public/app/js"
   :asset-path "/app/js"
   :modules {:main
             {:init-fn samurai-ui.app/init}}
   }

  :bootstrap-support
  {:target :bootstrap
   :output-dir "public/bootstrap"
   :exclude #{cljs.js}
   :entries [cljs.js reagent.core]
   :macros []}
  }
 }

